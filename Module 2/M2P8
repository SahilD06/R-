housing <- read.csv("csv9.csv")

colnames(housing)

housing$basement <- as.factor(housing$basement)
housing$furnishingstatus <- as.factor(housing$furnishingstatus)

anova_two <- aov(price ~ basement * furnishingstatus, data = housing)
summary(anova_two)

interaction.plot(
  housing$basement,
  housing$furnishingstatus,
  housing$price,
  xlab = "Basement (0 = No, 1 = Yes)",
  ylab = "Mean House Price",
  trace.label = "Furnishing Status",
  main = "Interaction Plot: Basement and Furnishing Status on Price",
  col = c("blue", "red", "green"),
  lty = 1:3,
  pch = c(19, 17, 15)
)
